{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "jsx": true,
    "modules": true
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "no-unused-vars": 0,
    "new-cap": 0,
    "semi": [
      2,
      "never"
    ]
  }
}
